  "performance_benchmarks": {
    "device_detection": {
      "iterations": 50,
      "concurrent_threads": 1,
      "timeout_seconds": 60,
      "expected_throughput_ops_per_second": 5.0,
      "max_memory_usage_mb": 200.0,
      "max_cpu_usage_percent": 50.0
