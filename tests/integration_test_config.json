  "reporting": {
    "formats": ["json", "html", "junit"],
    "include_performance_metrics": true,
    "include_system_info": true,
    "include_requirements_coverage": true,
    "generate_charts": false,
    "output_directory": "test_results",
    "archive_results": true,
    "max_archived_results": 10
  },
  
  "ci_cd_integration": {
    "jenkins": {
      "junit_xml_output": true,
      "performance_thresholds": true,
      "fail_on_coverage_below": 80.0
    },
    "github_actions": {
      "artifact_upload": true,
      "status_checks": true,
      "comment_on_pr": true
    },
    "docker": {
      "test_container_image": "forensics-toolkit-test:latest",
      "mount_volumes": ["/tmp/test_data", "/tmp/test_results"],
      "environment_variables": {
        "TEST_ENV": "ci",
        "MOCK_DEVICES": "true",
        "PERFORMANCE_TESTS": "false"
      }
    }
  }
